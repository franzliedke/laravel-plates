{
    "name": "franzl/laravel-plates",
    "description": "The powerful native PHP templating system Plates for Laravel.",
    "authors": [
        {
            "name": "Franz Liedke",
            "email": "franz@develophp.org"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "~5",
        "illuminate/view": "~5",
        "league/plates": "~3.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~1.5",
        "phpunit/phpunit": "~4.2"
    },
    "autoload": {
        "psr-4": {
            "Franzl\\LaravelPlates\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "stable"
}
